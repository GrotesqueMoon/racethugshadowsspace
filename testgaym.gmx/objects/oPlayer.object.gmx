            <string>/*Movement*/
MySpeed = 300 / room_speed


if keyboard_check(ord('W')) {

    y=y - MySpeed


}

if keyboard_check(ord('S')) {

    y=y + MySpeed


}

if keyboard_check(ord('A')) {

    x=x - MySpeed


}

if keyboard_check(ord('D')) {

    x=x + MySpeed


}

if keyboard_check(ord(vk_space)) {

    x=random (10)


}

/*Rotation and shooting*/

image_angle = point_direction(x,y,mouse_x,mouse_y)

if mouse_check_button(mb_left) and timer=0 {
    MyBullet = instance_create(x, y, oBullet)
    MyBullet.direction = point_direction(x, y, mouse_x, mouse_y)
    MyBullet.speed = 600 / room_speed
    timer = 5
    audio_play_sound(sound3,0.5,false)
/*Be kicked*/
    Kick = 300 / room_speed
    KickDirection = image_angle + 180
    x = x + lengthdir_x (Kick, KickDirection)
    y = y + lengthdir_y (Kick, KickDirection)
}

if timer &gt; 0 timer -=1

/*Stay in the room*/
if x &lt; 0 {x = 0}
if y &lt; 0 {y = 0}
if x &gt; room_width {x = room_width}
if y &gt; room_height {y = room_height}

/*Death*/
EnemyTouchAngle = instance_place (x,y, oEnemy)
if instance_exists (EnemyTouchAngle) {
    BitsCreated = 0
    
    
    while BitsCreated &lt; oGame.TotalBits {
    NewBit = instance_create (x,y,oPlayerChunk)
    BitsCreated = BitsCreated + 1
    }
    instance_create (x,y,oExplosion)
    audio_play_sound(vineboom,1,false)
    instance_destroy ()
}

    

</string>
